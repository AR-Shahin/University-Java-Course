<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coursefx.HelloController">
   <children>
      <Label layoutX="266.0" layoutY="49.0" text="Login" />
      <Label layoutX="129.0" layoutY="138.0" text="Name" />
      <Label layoutX="129.0" layoutY="178.0" text="Email" />
      <TextField fx:id="nameField" layoutX="199.0" layoutY="133.0" prefHeight="36.0" prefWidth="272.0" promptText="enter your name" />
      <TextField fx:id="emailField" layoutX="199.0" layoutY="176.0" prefHeight="36.0" prefWidth="272.0" promptText="enter your email" />
      <Button layoutX="199.0" layoutY="272.0" mnemonicParsing="false" onAction="#handleSubmitButton" text="Submit" />
      <Label layoutX="130.0" layoutY="223.0" text="Phone" />
      <TextField fx:id="phoneField" layoutX="197.0" layoutY="220.0" prefHeight="36.0" prefWidth="272.0" promptText="enter your phone" />
      <TextArea fx:id="display" layoutX="351.0" layoutY="285.0" prefHeight="130.0" prefWidth="200.0" />
      <ButtonBar layoutX="320.0" layoutY="45.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#changeWindow" text="Change" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
